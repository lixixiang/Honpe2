<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/ll_view0"
                android:layout_width="@dimen/_100dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/_5dp"
                android:layout_marginRight="@dimen/_5dp"
                android:gravity="center"
                android:padding="@dimen/_1dp">

                <TextView
                    android:id="@+id/view0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="起始日期"
                    android:textColor="@color/white_a5"
                    android:textSize="16sp" />
            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_05dp"
                android:layout_alignParentBottom="true"
                android:background="@color/white_a5" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/ll_view0"
                android:focusable="true"
                android:focusableInTouchMode="true">

                <TextView
                    android:id="@+id/et_startTime"
                    android:layout_width="match_parent"
                    android:layout_height="?android:attr/actionBarSize"
                    android:layout_gravity="center_vertical"
                    android:layout_marginEnd="@dimen/_40dp"
                    android:background="@android:color/transparent"
                    android:fontFamily="sans-serif"
                    android:gravity="center_vertical"
                    android:hint="如:2017-10-10"
                    android:padding="@dimen/_1dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textColorHint="@color/white_a5"
                    android:textCursorDrawable="@drawable/shape_black"
                    android:textSize="16sp" />

                <com.example.lxx.myalipay.widget.font.FontTextView
                    android:id="@+id/icon_font_delete0"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|right"
                    android:layout_marginStart="@dimen/_5dp"
                    android:layout_marginEnd="@dimen/_8dp"
                    android:gravity="center_vertical"
                    android:textSize="25sp"
                    android:text="&#xe78c;"
                    android:visibility="invisible" />

            </FrameLayout>
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ll_view1"
                android:layout_width="@dimen/_100dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/_5dp"
                android:layout_marginRight="@dimen/_5dp"
                android:gravity="center"
                android:padding="@dimen/_1dp">

                <TextView
                    android:id="@+id/view1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="结束日期"
                    android:textColor="@color/white_a5"
                    android:textSize="16sp" />

            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_05dp"
                android:layout_alignParentBottom="true"
                android:background="@color/white_a5" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/ll_view1"
                android:focusable="true"
                android:focusableInTouchMode="true">

                <TextView
                    android:id="@+id/et_overTime"
                    android:layout_width="match_parent"
                    android:layout_height="?android:attr/actionBarSize"
                    android:layout_gravity="center_vertical"
                    android:layout_marginEnd="@dimen/_40dp"
                    android:background="@android:color/transparent"
                    android:fontFamily="sans-serif"
                    android:gravity="center_vertical"
                    android:hint="如:2017-10-18"
                    android:padding="@dimen/_1dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textColorHint="@color/white_a5"
                    android:textCursorDrawable="@drawable/shape_black"
                    android:textSize="16sp" />

                <com.example.lxx.myalipay.widget.font.FontTextView
                    android:id="@+id/icon_font_delete1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|right"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:layout_marginRight="@dimen/_8dp"
                    android:gravity="center_vertical"
                    android:textSize="25sp"
                    android:text="&#xe78c;"
                    android:visibility="invisible" />

            </FrameLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/re_car_no"
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/ll_view2"
                android:layout_width="@dimen/_100dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/_5dp"
                android:layout_marginRight="@dimen/_5dp"
                android:gravity="center"
                android:padding="@dimen/_1dp">

                <TextView
                    android:id="@+id/view3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="派车状态"
                    android:textColor="@color/white_a5"
                    android:textSize="16sp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_05dp"
                android:layout_alignParentBottom="true"
                android:background="@color/white_a5" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/ll_view2"
                android:focusable="true"
                android:focusableInTouchMode="true">

                <TextView
                    android:id="@+id/edit_car_no"
                    android:layout_width="match_parent"
                    android:layout_height="?android:attr/actionBarSize"
                    android:layout_gravity="center_vertical"
                    android:layout_marginEnd="@dimen/_40dp"
                    android:background="@android:color/transparent"
                    android:fontFamily="sans-serif"
                    android:gravity="center_vertical"
                    android:hint="查看派车状态"
                    android:padding="@dimen/_1dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textColorHint="@color/white_a5"
                    android:textCursorDrawable="@drawable/shape_black"
                    android:textSize="16sp" />

                <com.example.lxx.myalipay.widget.font.FontTextView
                    android:id="@+id/icon_font_delete3"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|right"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:layout_marginRight="@dimen/_8dp"
                    android:gravity="center_vertical"
                    android:textSize="25sp"
                    android:text="&#xe78c;"
                    android:visibility="invisible" />

            </FrameLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/re_remark"
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            android:focusable="true"
            android:visibility="gone"
            android:focusableInTouchMode="true"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/ll_view3"
                android:layout_width="@dimen/_100dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/_5dp"
                android:layout_marginRight="@dimen/_5dp"
                android:gravity="center"
                android:padding="@dimen/_1dp">

                <TextView
                    android:id="@+id/view4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="派车状态"
                    android:textColor="@color/white_a5"
                    android:textSize="16sp" />

            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_05dp"
                android:layout_alignParentBottom="true"
                android:background="@color/white_a5" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/ll_view3"
                android:focusable="true"
                android:focusableInTouchMode="true">

                <EditText
                    android:id="@+id/et_remark"
                    android:layout_width="match_parent"
                    android:layout_height="?android:attr/actionBarSize"
                    android:layout_gravity="center_vertical"
                    android:layout_marginEnd="@dimen/_40dp"
                    android:background="@android:color/transparent"
                    android:fontFamily="sans-serif"
                    android:gravity="center_vertical"
                    android:hint="查看派车状态"
                    android:padding="@dimen/_1dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textColorHint="@color/white_a5"
                    android:textCursorDrawable="@drawable/shape_black"
                    android:textSize="16sp" />

                <com.example.lxx.myalipay.widget.font.FontTextView
                    android:id="@+id/icon_font_delete4"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|right"
                    android:layout_marginLeft="@dimen/_5dp"
                    android:layout_marginRight="@dimen/_8dp"
                    android:textSize="25sp"
                    android:gravity="center_vertical"
                    android:text="&#xe78c;"
                    android:visibility="invisible" />

            </FrameLayout>
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>